  "source": "src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.module.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.module.js"
    },
    "./dist/index.css": "./dist/index.css",
    "./dist/gatsby-wrapper.js": "./dist/gatsby-wrapper.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "jaen": "./dist/index.cli.mjs"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "microbundle src/index.ts --format cjs,esm --no-compress",
    "dev": "yarn build --watch",
    "build:cli": "microbundle -i src/cli/index.ts -o dist/index.cli.mjs --no-pkg-main --target node --generateTypes false --format esm",
    "dev:cli": "yarn build:cli --watch",
    "cli": "node ./dist/index.cli.mjs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "sq-gen": "snek-query generate https://services.snek.at/origin/graphql -o ./src/origin-api"
