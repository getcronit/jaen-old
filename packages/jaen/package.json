{
  "name": "@snek-at/jaen",
  "version": "0.0.26",
  "source": "src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.module.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.module.js"
    },
    "./dist/index.css": "./dist/index.css",
    "./package.json": "./package.json"
  },
  "bin": {
    "jaen": "./dist/index.cli.cjs"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "microbundle src/index.ts --format cjs,esm",
    "dev": "yarn build --watch",
    "build:cli": "microbundle -i src/cli/index.ts -o dist/index.cli.mjs --no-pkg-main --target node --generateTypes false --format esm",
    "dev:cli": "yarn build:cli --watch",
    "cli": "node ./dist/index.cli.mjs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "sq-gen": "snek-query generate https://services.snek.at/origin/graphql -o ./src/origin-api"
  },
  "repository": "https://github.com/snek-at/jaen",
  "author": "Nico Schett <schett@snek.at>",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/icons": "^2.0.12",
    "@chakra-ui/pro-theme": "^0.0.64",
    "@chakra-ui/react": "^2.3.6",
    "@ckeditor/ckeditor5-build-balloon": "^35.3.2",
    "@ckeditor/ckeditor5-react": "^5.0.3",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@jaenjs/snek-finder": "^1.0.0-beta.13",
    "@reach/router": "^1.3.4",
    "@reduxjs/toolkit": "^1.8.6",
    "@snek-functions/origin": "^0.2.2",
    "commander": "^9.4.1",
    "deepmerge": "^4.2.2",
    "form-data": "^4.0.0",
    "framer-motion": "^7.6.1",
    "jaen-utils": "*",
    "node-fetch": "^3.2.10",
    "pino": "^8.6.1",
    "pino-pretty": "^9.1.1",
    "rc-tree": "^5.7.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.39.5",
    "react-icons": "^4.6.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "redux": "^4.2.0",
    "snek-query": "*",
    "uuid": "^9.0.0",
    "vanilla-cookieconsent": "^2.8.9"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@chakra-ui/storybook-addon": "^4.0.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^8.3.4",
    "babel-loader": "^8.2.5",
    "microbundle": "^0.15.1"
  },
  "peerDependencies": {
    "gatsby": "^5.4.1",
    "gatsby-plugin-image": "^3.4.0",
    "gatsby-plugin-jaen": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
